generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int      @id @default(autoincrement())
  email String @unique
  role Type
}

enum Type {
  comercial
  prospect
}

model Lead {
  id          Int      @id @default(autoincrement())
  companyName String   @unique
  cep         String
  address     String
  email       String   @unique
  phone       String
  contact     String
  message     String?
  status      Boolean?
}
