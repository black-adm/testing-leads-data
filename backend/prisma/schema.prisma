generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id          Int     @id @default(autoincrement())
  companyName String  @unique
  cep         String
  address     String
  email       String  @unique
  phone       String
  contact     String
  message     String?
  status      Boolean?
}
